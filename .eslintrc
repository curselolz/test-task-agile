{
  
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jasmine": true
  },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "standard",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react-hooks"
    ],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}